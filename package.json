{
  "name": "@stoplight/elements",
  "version": "0.0.0",
  "description": "UI components for composing beautiful developer documentation.",
  "keywords": [],
  "sideEffects": false,
  "homepage": "https://github.com/stoplightio/elements",
  "bugs": "https://github.com/stoplightio/elements/issues",
  "author": "Stoplight <support@stoplight.io>",
  "repository": {
    "type": "git",
    "url": "https://github.com/stoplightio/elements"
  },
  "license": "Apache-2.0",
  "main": "",
  "files": [
    "**/*"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "yarn build.components",
    "postbuild": "yarn build.styles",
    "build.components": "sl-scripts build",
    "build.styles": "copyfiles -u 1 \"./src/styles/**/*\" ./dist",
    "build.docs": "build-storybook -c .storybook -o docs-auto",
    "commit": "git-cz",
    "lint": "yarn lint.prod --cache",
    "lint.prod": "eslint 'src/**/*.{ts,tsx}'",
    "lint.fix": "yarn lint --fix",
    "release": "sl-scripts release",
    "release.docs": "sl-scripts release:docs",
    "release.dryRun": "sl-scripts release --dry-run --debug",
    "storybook": "start-storybook -p 9001",
    "test": "jest",
    "test.prod": "yarn lint && yarn test --coverage --maxWorkers=2",
    "test.update": "yarn test --updateSnapshot",
    "test.watch": "yarn test --watch"
  },
  "peerDependencies": {
    "@stoplight/prism-http": "^4.0.0-beta.12",
    "mobx": ">=5",
    "react": ">=16.8",
    "react-dom": ">=16.8",
    "vis-network": "^7.5.2"
  },
  "dependencies": {
    "@stoplight/json": "^3.8.2",
    "@stoplight/json-ref-resolver": "^3.0.9",
    "@stoplight/json-schema-viewer": "^3.0.0-beta.28",
    "@stoplight/markdown": "^2.9.0",
    "@stoplight/markdown-viewer": "^4.2.1",
    "@stoplight/path": "^1.3.1",
    "@stoplight/react-error-boundary": "^1.0.0",
    "@stoplight/tree-list": "^5.0.3",
    "@stoplight/types": "^11.9.0",
    "@stoplight/ui-kit": "^3.0.0-beta.19",
    "@stoplight/yaml": "^4.2.0",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.3.1",
    "fast-text-encoding": "^1.0.1",
    "fp-ts": "^2.5.3",
    "graphql": "^14",
    "httpsnippet": "github:stoplightio/httpsnippet#master",
    "lodash": "^4.17.15",
    "mobx-react-lite": "^1.5.2",
    "object-hash": "^2.0.3",
    "openapi-sampler": "^1.0.0-beta.15",
    "parse-prefer-header": "^1.0.0",
    "react-error-boundary": "^1.2.5",
    "tslib": "^1.12.0",
    "type-is": "^1.6.18",
    "unist-util-select": "^3.0.1",
    "urijs": "^1.19.2",
    "urql": "^1.9.6"
  },
  "devDependencies": {
    "@stoplight/eslint-config": "^1.1.0",
    "@stoplight/prism-http": "^4.0.0-beta.12",
    "@stoplight/scripts": "8.2.1",
    "@stoplight/storybook-config": "^2.0.5",
    "@testing-library/react": "^10.0.2",
    "@types/classnames": "^2.2.10",
    "@types/enzyme": "3.x.x",
    "@types/enzyme-adapter-react-16": "1.x.x",
    "@types/jest": "25.2.1",
    "@types/lodash": "^4.14.149",
    "@types/object-hash": "^1.3.1",
    "@types/parse-prefer-header": "^1.0.1",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.x.x",
    "@types/type-is": "^1.6.3",
    "@types/urijs": "^1.19.8",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "copyfiles": "^2.2.0",
    "enzyme": "3.x.x",
    "enzyme-adapter-react-16": "1.x.x",
    "enzyme-to-json": "3.x.x",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "eslint-plugin-simple-import-sort": "^5.0.2",
    "jest": "25.x.x",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jest-enzyme": "7.x.x",
    "jest-fetch-mock": "^3.0.3",
    "mobx": "^5.13.0",
    "prettier": "^2.0.4",
    "ts-jest": "25.3.1",
    "typescript": "3.9.3",
    "vis-data": "^6.5.1",
    "vis-network": "^7.5.2",
    "vis-util": "^4.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "release": {
    "extends": "@stoplight/scripts/release"
  }
}
