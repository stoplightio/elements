{
  "name": "@stoplight/elements",
  "version": "0.0.0",
  "description": "UI components for composing beautiful developer documentation.",
  "keywords": [],
  "sideEffects": false,
  "homepage": "https://github.com/stoplightio/elements",
  "bugs": "https://github.com/stoplightio/elements/issues",
  "author": "Stoplight <support@stoplight.io>",
  "repository": {
    "type": "git",
    "url": "https://github.com/stoplightio/elements"
  },
  "license": "Apache-2.0",
  "main": "src/index.ts",
  "files": [
    "**/*"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "yarn build.components",
    "postbuild": "yarn build.styles",
    "build.components": "sl-scripts build",
    "build.styles": "copyfiles -u 1 \"./src/styles/**/*\" ./dist && yarn build.sass",
    "build.sass": "node-sass --output-style compressed --importer node_modules/node-sass-package-importer/dist/cli.js --include-path src/styles src/styles/elements.scss -o dist/styles",
    "build.docs": "build-storybook -c .storybook -o docs-auto",
    "commit": "git-cz",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src",
    "lint.fix": "eslint --ext .ts,.tsx,.js,.jsx --fix src",
    "release": "sl-scripts release",
    "release.docs": "sl-scripts release:docs",
    "release.dryRun": "sl-scripts release --dry-run --debug",
    "storybook": "start-storybook -p 9001",
    "test": "jest",
    "test.prod": "yarn type-check && yarn lint && yarn test --coverage --maxWorkers=2",
    "test.update": "yarn test --updateSnapshot",
    "test.watch": "yarn test --watch",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "@stoplight/prism-http": "^4.0.0-beta.5",
    "mobx": ">=5",
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "dependencies": {
    "@stoplight/json": "^3.6.0",
    "@stoplight/json-ref-resolver": "^3.0.8",
    "@stoplight/json-schema-viewer": "^3.0.0-beta.23",
    "@stoplight/markdown": "^2.7.2",
    "@stoplight/markdown-viewer": "^3.8.1",
    "@stoplight/path": "^1.3.1",
    "@stoplight/tree-list": "^5.0.3",
    "@stoplight/types": "^11.6.0",
    "@stoplight/ui-kit": "^2.15.1",
    "@stoplight/yaml": "^3.7.1",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.2.1",
    "fast-text-encoding": "^1.0.0",
    "fp-ts": "^2.5.2",
    "httpsnippet": "github:stoplightio/httpsnippet#master",
    "lodash": "^4.17.15",
    "mobx-react-lite": "^1.5.2",
    "object-hash": "^2.0.3",
    "openapi-sampler": "^1.0.0-beta.15",
    "parse-prefer-header": "^1.0.0",
    "react-error-boundary": "^1.2.5",
    "react-graph-vis": "^1.0.5",
    "swr": "^0.1.17",
    "type-is": "^1.6.18",
    "unist-util-select": "^3.0.1",
    "urijs": "^1.19.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@stoplight/eslint-config": "^1.1.0",
    "@stoplight/prism-http": "^4.0.0-beta.5",
    "@stoplight/scripts": "8.2.0",
    "@stoplight/storybook-config": "^2.0.5",
    "@testing-library/react": "^10.0.2",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "3.x.x",
    "@types/enzyme-adapter-react-16": "1.x.x",
    "@types/jest": "25.1.3",
    "@types/lodash": "^4.14.149",
    "@types/object-hash": "^1.3.1",
    "@types/parse-prefer-header": "^1.0.1",
    "@types/react": "16.9.23",
    "@types/react-dom": "16.x.x",
    "@types/type-is": "^1.6.3",
    "@types/urijs": "^1.19.5",
    "@types/vis": "^4.21.19",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "copyfiles": "^2.2.0",
    "css-loader": "^3.4.2",
    "enzyme": "3.x.x",
    "enzyme-adapter-react-16": "1.x.x",
    "enzyme-to-json": "3.x.x",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "eslint-plugin-simple-import-sort": "^5.0.2",
    "extract-loader": "^4.0.3",
    "file-loader": "^5.0.2",
    "jest": "25.x.x",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jest-enzyme": "7.x.x",
    "jest-fetch-mock": "^3.0.1",
    "mobx": "^5.13.0",
    "node-sass": "^4.13.1",
    "node-sass-package-importer": "^5.3.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.2",
    "resolve-url-loader": "^3.1.1",
    "sass-inline-svg": "^1.2.3",
    "sass-loader": "^7.3.1",
    "style-loader": "^1.1.3",
    "ts-jest": "25.2.1",
    "typescript": "3.7.x"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "extends": "@stoplight/scripts/release"
  }
}
