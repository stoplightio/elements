{
  "name": "@stoplight/elements",
  "version": "0.0.0",
  "description": "UI components for composing beautiful developer documentation.",
  "keywords": [],
  "sideEffects": false,
  "homepage": "https://github.com/stoplightio/elements",
  "bugs": "https://github.com/stoplightio/elements/issues",
  "author": "Stoplight <support@stoplight.io>",
  "repository": {
    "type": "git",
    "url": "https://github.com/stoplightio/elements"
  },
  "license": "Apache-2.0",
  "main": "src/index.ts",
  "files": [
    "**/*"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "yarn build.components && yarn build.styles && yarn build.widgets",
    "build.components": "sl-scripts build",
    "build.widgets": "NODE_ENV=production webpack -p --config webpack.config.js",
    "build.widgets.analyze": "RUN_CONTEXT=analyze NODE_ENV=production webpack -p --config webpack.config.js",
    "build.styles": "copyfiles -u 1 \"./src/styles/**/*\" ./dist",
    "build.docs": "build-storybook -c .storybook -o docs-auto",
    "commit": "git-cz",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx,js,jsx}'",
    "lint.fix": "yarn lint --fix",
    "release": "sl-scripts release",
    "release.docs": "sl-scripts release:docs",
    "release.dryRun": "sl-scripts release --dry-run --debug",
    "storybook": "start-storybook -p 9001",
    "test": "jest",
    "test.prod": "yarn lint && yarn test --coverage --maxWorkers=2",
    "test.update": "yarn test --updateSnapshot",
    "test.watch": "yarn test --watch"
  },
  "peerDependencies": {
    "mobx": ">=5",
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "dependencies": {
    "@rehooks/component-size": "^1.0.3",
    "@stoplight/json": "^3.2.1",
    "@stoplight/json-ref-resolver": "^3.0.3",
    "@stoplight/json-schema-viewer": "^2.10.4",
    "@stoplight/markdown": "^2.5.2",
    "@stoplight/markdown-viewer": "^3.7.0",
    "@stoplight/path": "^1.3.0",
    "@stoplight/request-maker": "^3.3.0",
    "@stoplight/tree-list": "^4.10.0",
    "@stoplight/types": "^11",
    "@stoplight/ui-kit": "^2.15.0",
    "@stoplight/yaml": "^3.3.2",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "lodash": "^4.17.15",
    "mobx-react-lite": "^1.5.1",
    "object-hash": "^2.0.0",
    "openapi-sampler": "^1.0.0-beta.15",
    "react-error-boundary": "^1.2.5",
    "react-graph-vis": "^1.0.5",
    "resize-observer-polyfill": "^1.5.1",
    "unist-util-select": "^2.0.2",
    "urijs": "^1.19.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@stoplight/scripts": "5.1.0",
    "@stoplight/storybook-config": "^2.0.4",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "3.x.x",
    "@types/enzyme-adapter-react-16": "1.x.x",
    "@types/jest": "24.x.x",
    "@types/lodash": "^4.14.149",
    "@types/object-hash": "^1.3.0",
    "@types/react": "16.x.x",
    "@types/react-dom": "16.x.x",
    "@types/typescript": "^2.0.0",
    "@types/urijs": "^1.19.4",
    "@types/vis": "^4.21.19",
    "autoprefixer": "^9.7.2",
    "babel-loader": "^8.0.6",
    "copyfiles": "^2.1.1",
    "css-loader": "^3.2.0",
    "enzyme": "3.x.x",
    "enzyme-adapter-react-16": "1.x.x",
    "enzyme-to-json": "3.x.x",
    "extract-loader": "^3.1.0",
    "file-loader": "^4.2.0",
    "jest": "24.x.x",
    "jest-enzyme": "7.x.x",
    "mobx": "^5.13.0",
    "nock": "^11.7.0",
    "node-sass": "^4.13.0",
    "node-sass-package-importer": "^5.3.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "1.x.x",
    "resolve-url-loader": "^3.1.1",
    "sass-inline-svg": "^1.2.3",
    "sass-loader": "^7.3.1",
    "style-loader": "^1.0.0",
    "ts-jest": "24.x.x",
    "tslint": "5.20.1",
    "tslint-config-stoplight": "1.4.0",
    "tslint-react": "4.1.x",
    "tslint-react-hooks": "2.2.x",
    "typescript": "3.7.2",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10"
  },
  "lint-staged": {
    "*.{ts,tsx}$": [
      "yarn lint.fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "release": {
    "extends": "@stoplight/scripts/release"
  }
}
