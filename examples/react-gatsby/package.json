{
  "name": "@stoplight/elements-starter-gatsby",
  "description": "A starter template to build beautiful developer docs using Stoplight Elements & Gatsby",
  "private": true,
  "version": "0.1.0",
  "author": "Stoplight <support@stoplight.io>",
  "license": "Unlicense",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stoplightio/elements-starter-gatsby.git"
  },
  "bugs": {
    "url": "https://github.com/stoplightio/elements-starter-gatsby/issues"
  },
  "scripts": {
    "reinstall": "rm -rf node_modules && yarn install",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "yarn develop",
    "serve": "gatsby serve -p 4200",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "deploy": "gatsby build && gh-pages -d public",
    "e2e:execute": "cypress run",
    "e2e:open": "cypress open",
    "e2e:run": "start-server-and-test serve http://localhost:4200 e2e:execute"
  },
  "resolutions": {
    "@stoplight/elements": "file:../../packages/elements/dist",
    "@stoplight/elements-utils": "file:../../packages/elements-utils/dist"
  },
  "dependencies": {
    "@stoplight/elements": "file:../../packages/elements/dist",
    "@stoplight/prism-http": "^4.0.1",
    "classnames": "^2.2.6",
    "gatsby": "^2.28.1",
    "gatsby-image": "^2.7.0",
    "gatsby-plugin-manifest": "^2.8.0",
    "gatsby-plugin-meta-redirect": "^1.1.1",
    "gatsby-plugin-offline": "^3.6.0",
    "gatsby-plugin-react-helmet": "^3.6.0",
    "gatsby-plugin-sharp": "^2.10.0",
    "gatsby-source-filesystem": "^2.7.0",
    "gatsby-transformer-sharp": "^2.8.0",
    "jsdom": "^16.4.0",
    "react": "^16.14.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/react-helmet": "^6.0.0",
    "canvas": "^2.6.1",
    "cypress": "^6.0.1",
    "gatsby-plugin-sass": "^2.3.12",
    "gh-pages": "^3.1.0",
    "node-sass": "^4.14.1",
    "node-sass-package-importer": "^5.3.2",
    "start-server-and-test": "^1.11.6",
    "typescript": "4.1.x"
  },
  "keywords": [
    "gatsby"
  ]
}
