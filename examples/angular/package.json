{
  "name": "@stoplight/elements-starter-angular",
  "version": "1.0.0",
  "description": "Example Angular project utilizing Stoplight Elements",
  "author": "Stoplight <support@stoplight.io>",
  "homepage": "https://github.com/stoplightio/elements-starter-angular#readme",
  "license": "Unlicense",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stoplightio/elements-starter-angular.git"
  },
  "scripts": {
    "reinstall": "rm -rf node_modules && yarn install",
    "start": "ng serve",
    "build": "ng build --prod",
    "postinstall": "ngcc --tsconfig './tsconfig.app.json'",
    "serve": "angular-http-server --path ./dist/angular -p 4200"
  },
  "bugs": {
    "url": "https://github.com/stoplightio/elements-starter-angular/issues"
  },
  "resolutions": {
    "@stoplight/elements": "file:../../packages/elements/dist",
    "@stoplight/elements-dev-portal": "file:../../packages/elements-dev-portal/dist",
    "@stoplight/elements-core": "file:../../packages/elements-core/dist",
    "@stoplight/elements-utils": "file:../../packages/elements-utils/dist"
  },
  "dependencies": {
    "@angular/animations": "^11.1.1",
    "@angular/common": "^11.1.1",
    "@angular/compiler": "^11.1.1",
    "@angular/core": "^11.1.1",
    "@angular/forms": "^11.1.1",
    "@angular/platform-browser": "^11.1.1",
    "@angular/platform-browser-dynamic": "^11.1.1",
    "@angular/router": "^11.1.1",
    "@stoplight/elements": "file:../../packages/elements/dist",
    "@stoplight/elements-dev-portal": "file:../../packages/elements-dev-portal/dist",
    "rxjs": "^6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular/cli": "^11.2.12",
    "@angular/compiler-cli": "^11.1.1",
    "@angular/language-service": "^12.0.1",
    "@angular-devkit/build-angular": "~0.1102.12",
    "@types/node": "^14.14.45",
    "angular-http-server": "~1.10.0",
    "typescript": "~4.1.5"
  }
}
